
.content {
	width: 100%;
	height: 500px;
	position: relative;
}
.main {
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
}
.btn {
	height: 80rpx;
	background-color: #007AFF;
	color: #fff;
	border-radius: 10rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 28rpx;
}

